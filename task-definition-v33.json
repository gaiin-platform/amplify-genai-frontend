{
  "family": "hfu-hfu-amplify-task",
  "executionRoleArn": "arn:aws:iam::135808927724:role/hfu-hfu-amplify-execution-role",
  "taskRoleArn": "arn:aws:iam::135808927724:role/hfu-hfu-amplify-task-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "containerDefinitions": [
    {
      "name": "hfu-amplifyApp",
      "image": "135808927724.dkr.ecr.us-east-1.amazonaws.com/hfu-hfu-amplify-repo:v34",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "NEXT_PUBLIC_DEFAULT_AUTH_PROVIDER",
          "value": "cognito"
        },
        {
          "name": "NEXT_PUBLIC_AUTH_ENABLED",
          "value": "true"
        },
        {
          "name": "NEXTAUTH_URL",
          "value": "https://hfu-amplify.org"
        },
        {
          "name": "NEXT_PUBLIC_APP_NAME",
          "value": "HFU Amplify"
        },
        {
          "name": "NEXT_PUBLIC_LLM_ROUTER_ENDPOINT",
          "value": "https://x18hnd6yh6.execute-api.us-east-1.amazonaws.com/prod/proxy/llm"
        },
        {
          "name": "NEXT_PUBLIC_API_BASE_URL",
          "value": "https://qfgrhljoh0.execute-api.us-east-1.amazonaws.com/prod"
        },
        {
          "name": "API_BASE_URL",
          "value": "https://qfgrhljoh0.execute-api.us-east-1.amazonaws.com/prod"
        },
        {
          "name": "CHAT_ENDPOINT",
          "value": "https://qfgrhljoh0.execute-api.us-east-1.amazonaws.com/prod/amplify/chat"
        },
        {
          "name": "COGNITO_ISSUER",
          "value": "https://cognito-idp.us-east-1.amazonaws.com/us-east-1_PgwOR439P"
        },
        {
          "name": "COGNITO_CLIENT_ID",
          "value": "2rq8ekafegrh5mcd51q80rt0bh"
        },
        {
          "name": "COGNITO_DOMAIN",
          "value": "https://amplify-prod-auth-1753145624.auth.us-east-1.amazoncognito.com"
        },
        {
          "name": "NEXT_TELEMETRY_DISABLED",
          "value": "1"
        }
      ],
      "secrets": [
        {
          "name": "NEXTAUTH_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:135808927724:secret:hfu-amplify/nextauth-secret-LMFx6l"
        },
        {
          "name": "COGNITO_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:135808927724:secret:hfu-amplify/cognito-client-secret-bPx5bx"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hfu-hfu-amplify-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true,
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:3000/api/health || exit 1"],
        "interval": 30,
        "timeout": 10,
        "retries": 5,
        "startPeriod": 120
      }
    }
  ]
}